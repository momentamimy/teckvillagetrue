<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/phoneNumbercontanierlay"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="?android:attr/selectableItemBackground"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <ImageView
        android:id="@+id/imagephonee"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_phone_gray_24dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="24dp"
        />


    <TextView
        android:id="@+id/contactuserphonenumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:gravity="center"
        android:layout_toEndOf="@+id/imagephonee"
        android:text="01280945456"
        android:textColor="#FF000000"
        android:layout_marginTop="20dp"
        android:textSize="15sp"
        android:layout_toRightOf="@+id/imagephonee" />

    <TextView
        android:id="@+id/typephone"
        android:layout_below="@+id/contactuserphonenumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/imagephonee"
        android:layout_marginLeft="20dp"
        android:gravity="center"
        android:text="Mobile"
        android:textSize="14sp"
        android:textColor="#FF000000"
        android:layout_marginBottom="20dp"
        android:layout_toRightOf="@+id/imagephonee" />



</RelativeLayout>