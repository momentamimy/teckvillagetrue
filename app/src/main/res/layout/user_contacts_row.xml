<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content">



    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/usercontactimg"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_nurse"
        />

    <TextView
        android:id="@+id/nameofcontacts"
        android:layout_toEndOf="@+id/usercontactimg"
        android:layout_toRightOf="@+id/usercontactimg"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Abdalla Kamash"
        android:textSize="15.5sp"
        android:textColor="@android:color/black"
        />



    <TextView
        android:id="@+id/country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PortSaid,Egypt"
        android:textSize="14sp"
        android:layout_marginTop="1dp"
        android:layout_alignStart="@+id/nameofcontacts"
        android:layout_below="@+id/nameofcontacts"

        />
</RelativeLayout>